package com.czdurham.tasty.core.events.tasting;

import java.util.UUID;

import com.czdurham.tasty.core.events.DeleteRequest;

public class TastingDeleteRequest extends DeleteRequest {
	private final UUID key;

	public TastingDeleteRequest(UUID key) {
		this.key = key;
	}

	public UUID getKey() {
		return key;
	}
}
