package com.czdurham.tasty.core.events;

import com.czdurham.tasty.core.domain.StatusDetails;

public class StatusEvent<S extends StatusDetails<?>> extends ReadEvent {
	private S status;
	
	protected StatusEvent() {
		setFound(false);
	}
	
	public StatusEvent(S status) {
		this.status = status;
	}
	
	public S getStatus() {
		return status;
	}
}
