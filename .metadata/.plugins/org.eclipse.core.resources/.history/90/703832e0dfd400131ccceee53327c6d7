package com.czdurham.tasty.core.events.tasting;

import java.util.UUID;

import com.czdurham.tasty.core.domain.tasting.TasterDetails;
import com.czdurham.tasty.core.events.CreatedEvent;

public class TastingCreatedEvent extends CreatedEvent {
	private final UUID key;
	private final TasterDetails tastingDetails;

	public TastingCreatedEvent(UUID key, TasterDetails tastingDetails) {
		this.key = key;
		this.tastingDetails = tastingDetails;
	}

	public UUID getKey() {
		return key;
	}

	public TasterDetails getTastingDetails() {
		return tastingDetails;
	}
}
