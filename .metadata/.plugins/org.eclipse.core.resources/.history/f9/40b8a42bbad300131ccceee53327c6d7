package com.czdurham.tasty.core.domain;

import java.util.Date;

public class TimestampedEnum<E extends Enum<?>> {
	private Date date;
	private E wnumValue;
	
	public TimestampedEnum(Date date, E status) {
		this.date = date;
		this.wnumValue = status;
	}
	
	public Date getDate() {
		return date;
	}
	
	public E getEnumValue() {
		return wnumValue;
	}
}
