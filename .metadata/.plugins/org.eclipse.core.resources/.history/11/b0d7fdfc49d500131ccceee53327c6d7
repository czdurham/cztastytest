package com.czdurham.tasty.core.domain;

import java.util.UUID;

public class BaseEntity {
	private UUID key;

	public BaseEntity() {
		this(UUID.randomUUID());
	}

	public BaseEntity(UUID key) {
		setKey(key);
	}

	public UUID getKey() {
		return key;
	}
	
	public void setKey(UUID key) {
		this.key = key;
	}
}
