package com.czdurham.tasty.events.tasting;

import java.util.UUID;

import com.czdurham.tasty.domain.tasting.TastingStatusDetails;
import com.czdurham.tasty.events.ReadEvent;

public class TastingStatusEvent extends ReadEvent {
	private UUID key;
	private TastingStatusDetails status;
	
	public TastingStatusEvent(UUID key) {
		this.key = key;
		setFound(false);
	}
	
	public TastingStatusEvent(UUID key, TastingStatusDetails) {
		this.key = key;
	}
	
	public static TastingStatusEvent notFound(UUID key) {
		return new TastingStatusEvent(key);
	}
}
