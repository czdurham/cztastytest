package com.czdurham.tasty.core.events.tasting;

import java.util.UUID;

import com.czdurham.tasty.core.events.PaginatedReadRequest;

public class TastingsDetailsRequest extends PaginatedReadRequest {
	private final UUID key;

	public TastingsDetailsRequest(UUID key) {
		this.key = key;
	}

	public UUID getKey() {
		return key;
	}
}
